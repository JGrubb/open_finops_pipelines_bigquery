# GCP Project Configuration
GCP_PROJECT_ID=your-gcp-project-id
GCP_REGION=us-central1

# Container Registry Configuration
ARTIFACT_REGISTRY_REPO=finops-pipelines
IMAGE_NAME=billing-pipeline

# AWS Billing Pipeline Configuration
AWS_GCS_BUCKET=your-gcs-bucket-name
AWS_GCS_PREFIX=gcs-transfer/aws_cur
AWS_TABLE_NAME=aws_cur_data
AWS_DATASET=aws_billing_data
AWS_SCHEDULE="0 2 * * *"

# Azure Billing Pipeline Configuration
AZURE_GCS_BUCKET=your-gcs-bucket-name
AZURE_GCS_PREFIX=gcs-transfer/azure/billingdata
AZURE_EXPORT_NAME=your-azure-export-name
AZURE_TABLE_NAME=azure_focus_billing_data
AZURE_DATASET=azure_billing_data
AZURE_SCHEDULE="0 3 * * *"

# Cloud Run Job Configuration (optional overrides)
# MEMORY=2Gi
# CPU=1
# TIMEOUT=1h
# MAX_RETRIES=2
